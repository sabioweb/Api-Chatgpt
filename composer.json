{
    "name": "sabioweb/api-chatgpt",
    "description": "A pure PHP OOP multi-model ChatGPT integration package supporting OCR, Mathematics, Programming, and Chat Bot models",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "SBAO",
            "email": "info@sabioweb.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SBAO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SBAO\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

